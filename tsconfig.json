{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "types": ["nativewind/types"],
    "baseUrl": ".",
    "paths": {
      "@ui/*": ["src/ui/*"],
      "@components/*": ["src/components/*"],
      "@theme/*": ["src/theme/*"],
      "@services/*": ["src/services/*"],
      "@api/*": ["src/api/*"],
      "@agent/*": ["src/agent/*"]
    }
  }
}
